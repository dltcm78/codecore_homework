<%- include("./partials/header") %>
<div class="container">
    <form method="POST" action="/cohorts">
        <div class="new_form">
            <div class="form-group">
                <label for="logo">Logo Url</label>
                <input name="logo" class="form-control" id="logo" placeholder="e.g. https://www.example.com/some.png">
            </div>
            <div class="form-group">
                <label for="name">Name<span id="warning"></span></label>
                <input name="name" class="form-control" id="name" placeholder="Flying Funks">
            </div>
            <div class="form-group">
                <label for="password">Password</label>
                <input name="password" class="form-control" id="password" placeholder="password">
            </div>
            <div class="form-group">
                <label for="members">Members</label>
                <textarea name="members" class="form-control" id="members"
                    placeholder="Jon Snow, Daenerys, Melisandre, Arya Stark"></textarea>
            </div></br>
            <button type="submit" class="btn btn-outline-primary float-right">Submit</button>
        </div>
    </form>
</div>
<%- include("./partials/footer") %>